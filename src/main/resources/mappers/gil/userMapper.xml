<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.happyjob.scm.gil.dao.UserMapper">

	<!-- loginID로 userInfo 가져오기  -->
	<select id="getUserInfoByLoginID" resultType="kr.happyjob.scm.gil.model.UserVO" >
		SELECT *
		FROM   tb_userinfo
		WHERE  loginID = #{loginID}
	</select>

</mapper>           